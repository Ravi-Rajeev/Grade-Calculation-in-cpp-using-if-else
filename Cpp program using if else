#include <iostream>
using namespace std;
int main(){
    int a1,a2,a3,a4,a5,per,total;
    cout<<"\t\tWelcome\n";
    cout<<"\t*****______________*****\t"<<endl;
    cout<<"Please enter your marks"<<endl;
    cout<<"\tPhysics:: "; cin>>a1;
    cout<<"\tChemistry:: "; cin>>a2;
    cout<<"\tMaths:: "; cin>>a3;
    cout<<"\tBiology:: "; cin>>a4;
    cout<<"\tComputer:: "; cin>>a5;
    total=a1+a2+a3+a4+a5;
    cout<<"\t----Total Marks----\t \n \t:-"<<total<<endl;
    per=(total*100)/500;
    cout<<"\t----Percentage-----\t \n \t:-"<<per<<"%"<<endl;
    
    if(per>=90){
        cout<<"Grade A"<<endl;
    }
    else if(per>=80&&per<90){
        cout<<"Grade B"<<endl;
    }
    else if(per>=70&&per<80){
        cout<<"Grade C"<<endl;
    }
    else if(per>=60&&per<70){
        cout<<"Grade D"<<endl;
    }
    else if(per>=50&&per<60){
        cout<<"Grade E"<<endl;
    }
    else if(per>=40&&per<50){
        cout<<"Grade F"<<endl;
    }
    else if(per<=39){
        cout<<"Fail"<<endl;
    }
     return 0;
}
